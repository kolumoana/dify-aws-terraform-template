# AWS Provider

aws_region = "ap-northeast-1"

default_tags = {
  Project = "kolumoana"
}

# S3 Bucket for Dify Storage

dify_storage_bucket = "dify-kolumoana-1"

# VPC

vpc_id             = "vpc-xxxxxxxxxxxxxxxxxx"
private_subnet_ids = ["subnet-xxxxxxxxxxxxxxxxx", "subnet-xxxxxxxxxxxxxxxxx"]
public_subnet_ids  = ["subnet-xxxxxxxxxxxxxxxxx", "subnet-xxxxxxxxxxxxxxxxx"]

redis_password     = "xxxxxxxxxxxxxxxxxxxxxxxxxxx"
db_master_password = "xxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Dify environment
migration_enabled = "true"
dify_db_password  = "xxxxxxxxxxxxxxxxxxxxxxxxxxx"

private_route_table_ids = ["rtb-xxxxxxxx"]
